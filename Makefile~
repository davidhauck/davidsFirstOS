all:
	nasm -f elf -o loader.o loader.s
	nasm -f elf -o gdt.o gdt.s
	nasm -f elf -o interrupt.o interrupt.s
	gcc -o isr.o -c isr.c -Wall -Wextra -Werror -nostdlib -nostartfiles -nodefaultlibs -ffreestanding	
	gcc -o kernel.o -c kernel.c -Wall -Wextra -Werror -nostdlib -nostartfiles -nodefaultlibs -ffreestanding	
	ld -T linker.ld -o kernel.bin loader.o kernel.o gdt.o interrupt.o isr.o
